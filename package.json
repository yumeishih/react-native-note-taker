  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/runtime": "7.4.0",
    "babel-jest": "24.5.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "24.5.0",
